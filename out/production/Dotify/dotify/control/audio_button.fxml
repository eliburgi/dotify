<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.shape.Arc?>
<?import javafx.scene.shape.ArcTo?>
<?import javafx.scene.shape.ClosePath?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.shape.LineTo?>
<?import javafx.scene.shape.MoveTo?>
<?import javafx.scene.shape.Path?>

<fx:root type="javafx.scene.layout.StackPane" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Arc centerX="30.0" centerY="30.0" fill="#ffffff00" length="360.0" radiusX="30.0" radiusY="30.0" stroke="#19b4a44a" strokeLineCap="ROUND" strokeLineJoin="ROUND" strokeWidth="4.0" />
      <Pane managed="false" prefHeight="64.0" prefWidth="64.0">
         <children>
              <Arc id="progressBar" centerX="32.0" centerY="32.0" fill="#ffffff00" length="90.0" managed="false" nodeOrientation="LEFT_TO_RIGHT" radiusX="30.0" radiusY="30.0" startAngle="90.0" stroke="#19b4a4" strokeLineCap="ROUND" strokeLineJoin="ROUND" strokeWidth="4.0">
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
            </Arc>
         </children>
      </Pane>
        <HBox id="stopIcon" alignment="CENTER" spacing="6.0" visible="false">
            <children>
                <Line id="freqBand3" endY="15.0" startY="-5.0" stroke="#19b4a4" strokeLineCap="ROUND" strokeLineJoin="ROUND" strokeWidth="5.0" />
                <Line id="freqBand3" endY="15.0" startY="-5.0" stroke="#19b4a4" strokeLineCap="ROUND" strokeLineJoin="ROUND" strokeWidth="5.0" />
            </children>
        </HBox>
        <Path id="playIcon" fill="#19b4a4" stroke="#19b4a4" strokeLineCap="ROUND" strokeWidth="3.0" visible="false">
            <elements>
                <MoveTo x="20.0" />
                <LineTo x="3.0" y="-10.0" />
                <LineTo x="3.0" y="10.0" />
                <ClosePath />
            </elements>
            <StackPane.margin>
                <Insets left="8.0" top="1.0" />
            </StackPane.margin>
        </Path>
      <Path id="replayIcon" fill="#1f93ff00" stroke="#19b4a4" strokeLineCap="ROUND" strokeLineJoin="ROUND" strokeWidth="4.0">
         <elements>
            <MoveTo x="10.0" />
            <ArcTo largeArcFlag="true" radiusX="12.0" radiusY="12.0" sweepFlag="true" y="8.0" />
            <LineTo absolute="false" x="5.0" y="5.0" />
            <MoveTo absolute="false" x="-5.0" y="-5.0" />
            <LineTo absolute="false" x="-5.0" y="5.0" />
         </elements>
      </Path>
    </children>
</fx:root>
